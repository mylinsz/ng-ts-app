{
  "extends": "../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "app",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "app",
      "kebab-case"
    ],
    "no-inferrable-types": [ // https://blog.csdn.net/mingyueyixi/article/details/72955218
      true,
      "ignore-params",
      "ignore-properties"
    ]
  }
}
